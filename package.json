  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.6",
    "@types/ws": "^8.5.5",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
